<form action="/" class="search-form">
    <input type="text" name="term" placeholder="Search for GIFs">
    <button type="submit">Search</button>
</form>
<form action="/" class="btn-container">
    <button type="submit" class="btn3 btn">Trending GIFs</Button>
</form>
<div class="container">
    {{#each gifs}}
    <div class="tenor-gif-embed" data-postid="{{this.id}}" data-share-method="host"
        data-aspect-ration="1.2266666666666666">
        <a href="{{this.itemurl}}">item Link</a>
        <a href="{{this.url}}">Url</a>
    </div>
    <script type="text/javascript" async src="https://tenor.com/embed.js"></script>
    {{/each}}
</div>